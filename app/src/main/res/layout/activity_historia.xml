<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_historia"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="appgps.com.appgps.HistoriaActivity">

    <Button
        android:text="Volver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/txtHistorial"
        android:layout_alignStart="@+id/txtHistorial"
        android:layout_marginTop="26dp"
        android:id="@+id/botonSalir" />

    <Spinner
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:id="@+id/spinnerHistorial"
        android:layout_below="@+id/botonSalir" />

    <com.google.android.gms.maps.MapView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/spinnerHistorial"
        android:layout_toEndOf="@+id/botonSalir"
        android:layout_marginTop="93dp"
        android:id="@+id/mapa"
        android:layout_toRightOf="@+id/botonSalir" />

</RelativeLayout>
